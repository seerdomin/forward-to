  "devDependencies": {
    "browserify": "^11.0.0",
    "eslint": "^0.24.1",
    "exorcist": "^0.4.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "sinon": "^1.15.4",
